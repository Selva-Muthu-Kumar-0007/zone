# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/
/tmp

#Ignore complied asset files if any
/public/assets

#Ignore sphinx compiled core files
/db/sphinx

#Ignore mobile for now
/mobile_cmd
/mobile
/hello

# Ignore All Redis DB Files
/*.rdb
.ruby-version

# Ignore db/schema file
/db/schema.rb


# Ignore all Unicorn tmp files
/pids/*

# Ignore all Vs Code files
/.vscode

.byebug_history

# Ignore RSpec Code Coverage Files
/coverage/*

# Ignore Local Gem Files
/vendor/bundle/*
